                                        }
                                    });
                                }

                                function updateAfterList(level, id) {
                                    $.ajax({
                                        type: "POST",
                                        contentType: 'application/json; charset=utf-8',
                                        url: "/weblogin/school/pSchoolList.aspx/RegisterGetSchool?t=" + (new Date().getTime()),
                                        data: "{'nvls':[{'name':'id','value':'" + id + "'}]}",
                                        dataType: "json",
