</style>
</head>
<body>
@Html.Partial("_SiteNav")
@Html.Partial("_Header2")
<div id="content">
    <input type="hidden" value="T6C62bS4Apux" id="J_TbToken">
    <div class="brandCon">
        <div class="userInfo">
            <ul class="links">
                <a href="http://brand.tmall.com">
                    <li>
                        <span>去品牌街逛逛</span> <i></i>
                    </li>
                </a>
                <a href="http://cart.tmall.com/cart/myCart.htm">
                    <li>
                        <span>购物车</span> <i></i>
                    </li>
                </a>
            </ul>

            <a class="userInfo-photo">
                <img src="http://wwc.taobaocdn.com/avatar/getAvatar.do?userId=70566442&amp;width=120&amp;height=120&amp;type=sns" height="48" width="48" alt="basilwang"></a>

            <div class="userInfo-text"> <strong>basilwang</strong>
                <p>
                    您共关注了 <em>@Model.FavMerchants.Count</em>
                    个品牌
                </p>
            </div>
        </div>

        @if (Model.FavMerchants != null)
        {
            var foodIndex = 0;
            var houseIndex = 0;
            var commodityIndex = 0;

            foreach (friday.core.Merchant m in Model.FavMerchants)
            {
                <div class="brandItem">
            <div class="brandItem-info">
                <div class="bIi-brand j_BrandItemInfo">
                    <p class="bIi-brand-logo">
                        <a target="_blank" href="http://list.tmall.com/search_product.htm?id=@m.Id">
                            <img height="45" width="90" alt="@m.Name" src="@m.Logo"></a> <strong>@m.Name</strong>
                    </p>
                </div>
                <p class="bIi-addBtn">
                    <a class="bIi-addBtn-active j_CancelCollectBtn" data-brandid="@m.Id">取消关注</a>
                </p>
            </div>
            <div class="brandItem-con j_BrandItemSlide">
                <div class="bIc-title">
                    <p class="bIc-title-notice">
                        <span>推荐:</span>
                        <a target="_blank" class="bIc-title-notice-shop" href="http://mlatin.tmall.com">@m.Name</a> <em>品牌直销</em>
                        <a target="_blank" href="http://mlatin.tmall.com"></a>
                    </p>

                    <a target="_blank" class="bIc-title-more" href="http://list.tmall.com/search_product.htm?style=w&amp;brand=10076545&amp;from=brandguide">
                        查看所有店铺
                        <i></i>
                    </a>
                </div>
                <a href="javascript:;" class="bIc-slide-prev">&lt;</a>
                <div class="bIc-slide">
                    <ul class="bIc-slideList" style="position: absolute; width: 2160px;">
                    @if (m.MerchantType == friday.core.EnumType.MerchantTypeEnum.餐馆)
                    {
                        for (var index = 0; index < (Model.Foods[foodIndex].Count() > 12 ? 12 : Model.Foods[foodIndex].Count()); index++)
                        {
                            var item = Model.Foods[foodIndex].ElementAt(index);
                        <li class="j_BrandItemList" style="display: block; float: left;">
                            <p class="bIc-slideList-img">
                                <a target="_blank" href="http://localhost:7525/Merchant/Detail?brandId=@item.Id">
                                    <img height="160" width="160" src="@item.Image"></a>
                            </p>
                            <p class="bIc-slideList-sell">
                                <span class="bIc-slideList-sell-price ui-price">
                                    <span class="ui-price-icon">¥</span>
                                    @item.Price
                                </span>
                                <span class="bIc-slideList-sell-num">
                                    月销量：
                                    <em>@item.MonthAmount</em>
                                </span>
                            </p>
                            <p class="bIc-slideList-title">
                                <a target="_blank" href="http://localhost:7525/Merchant/Detail?brandId=@item.Id">@item.Name</a>
                            </p>
                        </li>
                        }
                        foodIndex++;
                    }
                    else if (m.MerchantType == friday.core.EnumType.MerchantTypeEnum.百货)
                    {
                        for (var index = 0; index < (Model.Commoditys[commodityIndex].Count() > 12 ? 12 : Model.Commoditys[commodityIndex].Count()); index++)
                        {
                            var item = Model.Commoditys[commodityIndex].ElementAt(index);
                        <li class="j_BrandItemList" style="display: block; float: left;">
                            <p class="bIc-slideList-img">
                                <a target="_blank" href="http://localhost:7525/Merchant/Detail?brandId=@item.Id">
                                    <img height="160" width="160" src="@item.Image"></a>
                            </p>
                            <p class="bIc-slideList-sell">
                                <span class="bIc-slideList-sell-price ui-price">
                                    <span class="ui-price-icon">¥</span>
                                    @item.Price
                                </span>
                                <span class="bIc-slideList-sell-num">
                                    月销量：
                                    <em>@item.MonthAmount</em>
                                </span>
                            </p>
                            <p class="bIc-slideList-title">
                                <a target="_blank" href="http://localhost:7525/Merchant/Detail?brandId=@item.Id">@item.Name</a>
                            </p>
                        </li>
                        }
                        commodityIndex++;
                    }
                    else
                    {
                        for (var index = 0; index < (Model.Houses[houseIndex].Count() > 12 ? 12 : Model.Houses[houseIndex].Count()); index++)
                        {
                            var item = Model.Houses[houseIndex].ElementAt(index);
                        <li class="j_BrandItemList" style="display: block; float: left;">
                            <p class="bIc-slideList-img">
                                <a target="_blank" href="http://localhost:7525/Merchant/Detail?brandId=@item.Id">
                                    <img height="160" width="160" src="@item.Image"></a>
                            </p>
                            <p class="bIc-slideList-sell">
                                <span class="bIc-slideList-sell-price ui-price">
                                    <span class="ui-price-icon">¥</span>
                                    @item.Price
                                </span>
                                <span class="bIc-slideList-sell-num">
                                    月销量：
                                    <em>@item.MonthAmount</em>
                                </span>
                            </p>
                            <p class="bIc-slideList-title">
                                <a target="_blank" href="http://localhost:7525/Merchant/Detail?brandId=@item.Id">@item.Name</a>
                            </p>
                        </li>
                        }
                        houseIndex++;
                    }
                        
                    </ul>
                </div>
                <a href="javascript:;" class="bIc-slide-next">&gt;</a>
            </div>
        </div>
            }
        }

        <div class="brandPagination">
            <div class="ui-page">
                <div class="ui-page-wrap"> <b class="ui-page-num">
                @if (Model.currenPage > 1)
                {
                    <a class="ui-page-prev j_PageChange " data-page="@(Model.currenPage-1)" href="#">&lt;&lt; 上一页</a>
                }
                else
                {
                    <b class="ui-page-prev">&lt;&lt; 上一页</b>
                }
                @for (int i = 1; i <= Model.pageNum; i++)
                {
                    if (i == Model.currenPage)
                    {
                        <b class="ui-page-cur">@i</b>
                    }
                    else
                    {
                        <a href="#" class="j_PageChange" data-page="@i">@i</a>
                    }
                }
                
                <b class="ui-page-break">...</b>

                @if (Model.currenPage < Model.pageNum)
                {
                    <a class="ui-page-next j_PageChange " data-page="@(Model.currenPage+1)" href="#">&lt;&lt; 下一页</a>
                }
                else
                {
                    <b class="ui-page-next">下一页 &gt;&gt;</b>
                }
                    </b>

                    <b class="ui-page-skip">共@(Model.pageNum)页，去第
                        <input type="text" value="@Model.currenPage" size="3" class="ui-page-skipTo j_PageChangeInput" name="jumpto">
                        页
                        <button class="ui-btn-s ui-page-skipBtn j_PageChangeBtn" type="submit">确定</button></b> 
                </div>
            </div>
        </div>

        <form id="J_BrandTagForm" method="post" style="clear:both">
            <input type="hidden" name="page" value="1"></form>

    </div>

    <script>
    KISSY.use('2.0/fav', function (S, UserFav) {
        new UserFav();
    });
</script>

</div>
</body>
</html>