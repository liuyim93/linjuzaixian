                        {
                         @:<a href="" target="_blank" >@Html.Encode(item.MerchantCategoryName)</a>
                        }
              
                    }
                    <b class="overlay even"></b>
                    </p>
                <s class="menuIcon"></s></li>
        </ul>
        <div class="menu-bottom j_BottomMenu">
            <a href="http://cz.tmall.com/?spm=3.356818.294515.155&amp;sort=phone" target="_blank">
                <h3 class="item-hd item-hd16">
                    <s></s>手机充值/网游点卡</h3>
                <s class="menuIcon"></s></a>
        </div>
        <s class="menuMask"></s>
    </div>
</div>
