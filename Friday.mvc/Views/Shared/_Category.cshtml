                        var item = Model.MerchantShopCategories.ElementAt(index);
                        if (index % 5 == 0)
                        {
                        @:<a href="" target="_blank" class="item-col1">@Html.Encode(item.MerchantCategoryName)</a>
                        }
                        else
                        {
                         @:<a href="" target="_blank" >@Html.Encode(item.MerchantCategoryName)</a>
                        }
              
                    }
                    <b class="overlay even"></b>
                    </p>
                <s class="menuIcon"></s></li>
            <li class="item item3 j_MenuItem">
                <h3 class="item-hd item-hd3">
                    <s></s>餐馆
                </h3>
                 @for (var index = 0; index < Model.MerchantRestaurantCategories.Count(); index++)
                    {
                        
                            
